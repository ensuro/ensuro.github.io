{% extends "master.j2" %}
      {% block main_content %}
      <!-- Hero Start  -->
      <!-- Pr preview -->
      <div  class="hero-section" id="home">
         <div class="hero-section-image">
            <div class="custom-container">
               <div class="hero-inner d-flex justify-content-between align-items-center flex-wrap">
                  <div class="hero-data col-lg-6 col-md-12">
                     <h1 class="hero-top animated" data-animation="fadeInDownC" data-animation-delay="200">
                        decentralized<br>
                        <b>Capital for Insurance</b>
                     </h1>
                     <p class="hero-dis   animated" data-animation="fadeInDownC" data-animation-delay="300">Ensuro is the first decentralized, licensed (re)insurer on a public blockchain. We use smart contracts to curate competitive insurance portfolios. We allow anyone to invest in insurance risk and reap its benefits. By opening up the insurance market to new players, we fuel innovation for established and upcoming insurance partners.</p>
                     <div class="d-flex mt-5 home-btn">
                        <a href="#get" class="btn-con page-scroll  animated" data-animation="fadeInDownC" data-animation-delay="400">Get Started</a>
                        <a href="#work" class="btn-con-border page-scroll   animated" data-animation="fadeInDownC" data-animation-delay="500">How It Works</a>
                     </div>
                  </div>
                  <div class="hero-img col-lg-6 col-md-12    animated" data-animation="fadeInDownC" data-animation-delay="200">
                     <img src="images/Illustration.svg" alt="Image" class="img-fluid"/>
                  </div>
               </div>
            </div>
         </div>
      </div>
      <!--Hero End  -->
      <!-- section start-->
      <div class="section" id="get">
         <div class="container">
            <!--Title -->
            <div class="sec-title    animated" data-animation="fadeInDownC" data-animation-delay="500">
               <h2 class="text-center">Ensuro brings the innovative DeFi Capital to the Insurtech World</h2>
            </div>
            <div class="icon-img d-flex justify-content-center    animated" data-animation="fadeInDownC" data-animation-delay="600">
               <img src="images/Icons.svg" alt="img" class="img-fluid i-big">
            </div>
            <div class="row mt-5">
               <!-- Item -->
               <div class="col-lg-4 col-md-12 col-sm-12 col-xs-12    animated" data-animation="fadeInDownC" data-animation-delay="700">
                  <div class="icon-img d-flex justify-content-center">
                     <img src="images/o1.png" alt="img" class="img-fluid i-s">
                  </div>
                  <div class="inner-box d-flex flex-wrap justify-content-center flex-column">
                     <div class="box-head">Risk Model Providers / MGAs</div>
                  </div>
               </div>
               <!-- Item -->
               <div class="col-lg-4 col-md-12 col-sm-12 col-xs-12   animated" data-animation="fadeInDownC" data-animation-delay="800">
                  <div class="icon-img d-flex justify-content-center">
                     <img src="images/line.png" alt="img" class="img-fluid i-s">
                  </div>
                  <div class="icon-img d-flex justify-content-center">
                     <img src="images/o2.png" alt="img" class="img-fluid i-s">
                  </div>
                  <div class="inner-box d-flex flex-wrap justify-content-center flex-column">
                     <div class="box-head">Ensuro Liquidity Pool</div>
                     <p>Permissionless Blockchain
                     </p>
                  </div>
               </div>
               <!-- Item -->
               <div class="col-lg-4 col-md-12 col-sm-12 col-xs-12    animated" data-animation="fadeInDownC" data-animation-delay="900">
                  <div class="icon-img d-flex justify-content-center">
                     <img src="images/line.png" alt="img" class="img-fluid i-s">
                  </div>
                  <div class="icon-img d-flex justify-content-center">
                     <img src="images/o3.png" alt="img" class="img-fluid i-s">
                  </div>
                  <div class="inner-box d-flex flex-wrap justify-content-center flex-column">
                     <div class="box-head">Liquidity Providers</div>
                  </div>
               </div>
            </div>
         </div>
      </div>
      <!-- section end-->
      <!-- section start-->
      <div class="section" id="work">
         <div class="container">
            <!--Title -->
            <div class="sec-title    animated" data-animation="fadeInDownC" data-animation-delay="200">
               <h2 class="text-center">How It Works</h2>
            </div>
            <div class="icon-img d-flex justify-content-center   animated" data-animation="fadeInDownC" data-animation-delay="300">
               <img src="images/1.svg" alt="img" class="img-fluid i-big">
            </div>
            <div class="row mt-5">
               <!-- Item -->
               <div class="col-lg-4 col-md-12 col-sm-12 col-xs-12   animated" data-animation="fadeInDownC" data-animation-delay="400">
                  <div class="icon-img d-flex justify-content-center">
                     <img src="images/o1.png" alt="img" class="img-fluid i-s">
                  </div>
                  <div class="inner-box d-flex flex-wrap justify-content-center flex-column">
                     <div class="box-head">Risk Model Providers/ MGAs</div>
                  </div>
               </div>
               <!-- Item -->
               <div class="col-lg-4 col-md-12 col-sm-12 col-xs-12    animated" data-animation="fadeInDownC" data-animation-delay="500">
                  <div class="icon-img d-flex justify-content-center">
                     <img src="images/arrow1.png" alt="img" class="img-fluid i-s">
                  </div>
                  <div class="icon-img d-flex justify-content-center">
                     <img src="images/o2.png" alt="img" class="img-fluid i-s">
                  </div>
                  <div class="inner-box d-flex flex-wrap justify-content-center flex-column">
                     <div class="box-head">Ensuro Liquidity Pool</div>
                     <p>Permissionless Blockchain
                     </p>
                  </div>
               </div>
               <!-- Item -->
               <div class="col-lg-4 col-md-12 col-sm-12 col-xs-12    animated" data-animation="fadeInDownC" data-animation-delay="600">
                  <div class="icon-img d-flex justify-content-center">
                     <img src="images/arrow2.png" alt="img" class="img-fluid i-s">
                  </div>
                  <div class="icon-img d-flex justify-content-center">
                     <img src="images/o3.png" alt="img" class="img-fluid i-s">
                  </div>
                  <div class="inner-box d-flex flex-wrap justify-content-center flex-column">
                     <div class="box-head">Liquidity Providers</div>
                  </div>
               </div>
            </div>
            <p class="text-center mt-5    animated" data-animation="fadeInDownC" data-animation-delay="700">
               Ensuro collects capital from Liquidity Providers, both retail and institutional, through a Liquidity Pool governed by smart contracts. This capital, collected in the form of cryptographic stablecoins, provides underwriting capacity for Insurtech companies operating in the parametric insurance space.
            </p>
         </div>
      </div>
      <!-- section end-->
      <!-- section start-->
      <div class="section">
         <div class="container">
            <!--Title -->
            <div class="icon-img d-flex justify-content-center   animated" data-animation="fadeInDownC" data-animation-delay="800">
               <img src="images/2.svg" alt="img" class="img-fluid i-big">
               <div class="icon-img d-flex justify-content-center">
                  <img src="images/6.svg" alt="img" class="img-fluid i-s">
               </div>
            </div>
            <div class="row mt-5 i-big">
               <!-- Item -->
               <div class="col-lg-4 col-md-12 col-sm-12 col-xs-12    animated" data-animation="fadeInDownC" data-animation-delay="900">
                  <div class="inner-box d-flex flex-wrap justify-content-center flex-column">
                     <div class="box-head">Risk Model Providers / MGAs</div>
                  </div>
               </div>
               <!-- Item -->
               <div class="col-lg-4 col-md-12 col-sm-12 col-xs-12    animated" data-animation="fadeInDownC" data-animation-delay="1000">
                  <div class="inner-box d-flex flex-wrap justify-content-center flex-column">
                     <div class="box-head">Ensuro Liquidity Pool</div>
                     <p>Permissionless Blockchain
                     </p>
                  </div>
               </div>
               <!-- Item -->
               <div class="col-lg-4 col-md-12 col-sm-12 col-xs-12    animated" data-animation="fadeInDownC" data-animation-delay="1100">
                  <div class="inner-box d-flex flex-wrap justify-content-center flex-column">
                     <div class="box-head">Liquidity Providers</div>
                  </div>
               </div>
            </div>
            <p class="text-center mt-5   animated" data-animation="fadeInDownC" data-animation-delay="1200">
             The capital collected in the Liquidity Pool is reinvested into Decentralized Finance Protocols such as AAVE and Compound. These protocols offer low risk due to overcollateralized loans, high liquidity, and greater returns on investment than traditional risk-free assets. Therefore, Ensuro can rely on instant redemption of funds and provide both a competitive capacity to Insurtech companies and additional returns to Liquidity Providers.
            </p>
         </div>
      </div>
      <!-- section end-->
      <!-- section start-->
      <div class="section">
         <div class="container">
            <!--Title -->
            <div class="icon-img d-flex justify-content-center    animated" data-animation="fadeInDownC" data-animation-delay="1200">
               <img src="images/3.svg" alt="img" class="img-fluid i-big">
            </div>
            <div class="icon-img d-flex justify-content-center    animated" data-animation="fadeInDownC" data-animation-delay="1200">
               <img src="images/5.svg" alt="img" class="img-fluid i-s">
            </div>
            <p class="text-center mt-5    animated" data-animation="fadeInDownC" data-animation-delay="1300">
             Ensuro leverages a Quality Assurance System to monitor the performance of each risk model, certifying that it fits the standard parameters defined during integration.
            </p>
         </div>
      </div>
      <!-- section end-->

      <!-- Partners carousel --> 
      <div class="section-bottom">
         <div class="container">
            <!--Title -->
            <div class="sec-title      animated" data-animation="fadeInDownC" data-animation-delay="1000">
               <h2 class="text-center">Partners And Investors</h2>
            </div>
            <!--Blog-inner Start-->
            <div class="blog-inner">
               <div class="swiffy-slider slider-item-show3 slider-item-reveal slider-nav-outside slider-nav-dark slider-nav-sm slider-nav-visible slider-indicators-outside slider-indicators-round slider-indicators-dark slider-nav-animation slider-nav-animation-fadein slider-item-first-visible  animated" data-animation="fadeInDownC" data-animation-delay="400">
                  <div class="slider-container">
                     <div>
                        <a href="https://sosv.com/" target="_blank" style="display:block">
                           <img src="images/logo-sosv.svg" alt="img" class="img-fluid p-4" width="100%">
                        </a>
                     </div>
                     <div>
                        <a href="https://www.artesianinvest.com/" target="_blank">
                           <img src="images/logo-artesian.svg" alt="img" class="img-fluid p-4" width="100%">
                        </a>
                     </div>
                     <div>
                        <a href="https://hikoala.co/" target="_blank">
                           <img src="images/logo-koala.png" alt="img" class="img-fluid p-4" width="100%">
                        </a>
                     </div>
                     <div>
                        <a href="https://polygon.technology/" target="_blank" style="display:block">
                           <img src="images/logo-polygon.png" alt="img" class="img-fluid p-4" width="100%">
                        </a>
                     </div>
                     <div>
                        <a href="https://iasv.co/" target="_blank">
                           <img src="images/logo-iaventures.png" alt="img" class="img-fluid p-4" width="100%">
                        </a>
                     </div>
                     <div>
                        <a href="https://orbitstartups.com/" target="_blank">
                           <img src="images/logo-orbit.png" alt="img" class="img-fluid p-4" width="100%">
                        </a>
                     </div>
                     <div>
                        <a href="https://byparachute.com/" target="_blank">
                           <img src="images/logo-parachute.png" alt="img" class="img-fluid p-4" width="100%">
                        </a>
                     </div>
                  </div>

                  <button type="button" class="slider-nav" aria-label="Go left"></button>
                  <button type="button" class="slider-nav slider-nav-next" aria-label="Go left"></button>
               </div>
            </div>
            <!--Blog-inner End-->
         </div>
      </div>
      <!-- End of Partners carousel-->

      <!-- Twitter carousel -->
      <div class="section">
         <div class="container">
            <!--Title -->
            <div class="sec-title    animated" data-animation="fadeInDownC" data-animation-delay="200">
               <h2 class="text-center">Twitter</h2>
            </div>
            <!--Blog-inner Start-->
            <div class="blog-inner">
               <div class="swiffy-slider slider-item-show2 slider-item-reveal slider-nav-outside slider-nav-round slider-nav-visible slider-indicators-outside slider-indicators-round slider-indicators-dark slider-nav-animation slider-nav-animation-fadein slider-item-first-visible  animated" data-animation="fadeInDownC" data-animation-delay="400">
                  <ul class="slider-container py-4">
                     {% for tw in tweets %}
                     <li class="slide-visible">
                        <div class="card mb-2" onclick="window.open('{{tw.link}}', '_blank');"  style="cursor: pointer;">
                           <div class="card-body">
                              <h2 class="card-title">
                                 <a href="https://twitter.com/Ensuroproject" target="_blank" class="twitter">
                                    <img width="24" height="24" viewBox="0 0 24 20" fill="none" src="images/ensuro-icon-64px.png"/>
                                 </a>
                                 Ensuro
                              </h2>
                              <h4 class="card-subtitle mb-2 text-muted">@Ensuroproject</h4>
                              <p class="card-text">{{tw.text}}</p>
                              <br></br>
                              <a href="https://twitter.com/Ensuroproject" target="_blank" class="twitter" style="float: right">
                                 <svg width="24" height="20" viewBox="0 0 24 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path d="M24 2.30948C23.1075 2.70098 22.1565 2.96048 21.165 3.08648C22.185 2.47748 22.9635 1.52048 23.3295 0.366976C22.3785 0.933976 21.3285 1.33448 20.2095 1.55798C19.3065 0.596477 18.0195 0.000976562 16.6155 0.000976562C13.8915 0.000976562 11.6985 2.21198 11.6985 4.92248C11.6985 5.31248 11.7315 5.68748 11.8125 6.04448C7.722 5.84498 4.1025 3.88448 1.671 0.897977C1.2465 1.63448 0.9975 2.47748 0.9975 3.38498C0.9975 5.08898 1.875 6.59948 3.183 7.47398C2.3925 7.45898 1.617 7.22948 0.96 6.86798C0.96 6.88298 0.96 6.90248 0.96 6.92198C0.96 9.31298 2.6655 11.299 4.902 11.7565C4.5015 11.866 4.065 11.9185 3.612 11.9185C3.297 11.9185 2.979 11.9005 2.6805 11.8345C3.318 13.783 5.127 15.2155 7.278 15.262C5.604 16.5715 3.4785 17.3605 1.1775 17.3605C0.774 17.3605 0.387 17.3425 0 17.293C2.1795 18.6985 4.7625 19.501 7.548 19.501C16.602 19.501 21.552 12.001 21.552 5.49998C21.552 5.28248 21.5445 5.07248 21.534 4.86398C22.5105 4.17098 23.331 3.30548 24 2.30948Z" fill="#01358F"/>
                                 </svg>
                              </a>
                           </div>
                        </div>
                     </li>
                     {% endfor %}
                  </ul>
                  
                  <button type="button" class="slider-nav" aria-label="Go left"></button>
                  <button type="button" class="slider-nav slider-nav-next" aria-label="Go left"></button>
            
                  <div class="slider-indicators">
                     <button class="active" aria-label="Go to slide"></button>
                     <button aria-label="Go to slide"></button>
                     <button aria-label="Go to slide"></button>
                     <button aria-label="Go to slide"></button>
                     <button aria-label="Go to slide"></button>
                  </div>
               </div>
            </div>
            <!--Blog-inner End-->
         </div>
      </div>
      <!-- End of Twitter carousel --> 
      
      <!-- Latest News carousel -->
      <div class="section">
         <div class="container">
            <!--Title -->
            <div class="sec-title    animated" data-animation="fadeInDownC" data-animation-delay="200">
               <h2 class="text-center">Latest News</h2>
            </div>
            <!--Blog-inner Start-->
            <div class="blog-inner">
               <div class="swiffy-slider slider-item-show2 slider-item-reveal slider-nav-outside slider-nav-round slider-nav-visible slider-indicators-outside slider-indicators-round slider-indicators-dark slider-nav-animation slider-nav-animation-fadein slider-item-first-visible  animated" data-animation="fadeInDownC" data-animation-delay="400">
                  <ul class="slider-container py-4">
                     {% for n in news %}
                     <li class="slide-visible">
                        <div class="card shadow h-100" onclick="window.open('{{n.link}}', '_blank');"  style="cursor: pointer;">
                           <div class="ratio ratio-16x9">
                              <img src="{{n.image}}" class="card-img-top" style="width:100%; height:300px;" loading="lazy">
                           </div>
                           <div class="card-body">
                              <span class="color-3cc91d fw-600">{{n.date}}</span>
                              <a href="{{n.link}}" target="_blank">
                                 <h3 class="card-text">{{n.title}}</h3>
                              </a>
                              <br><br/>
                              <div><a href="{{n.link}}" target="_blank">Read more</a></div>
                           </div>
                        </div>
                     </li>
                     {% endfor %}
                  </ul>
                  
                  <button type="button" class="slider-nav" aria-label="Go left"></button>
                  <button type="button" class="slider-nav slider-nav-next" aria-label="Go left"></button>
            
                  <div class="slider-indicators">
                     <button class="active" aria-label="Go to slide"></button>
                     <button aria-label="Go to slide"></button>
                     <button aria-label="Go to slide"></button>
                     <button aria-label="Go to slide"></button>
                     <button aria-label="Go to slide"></button>
                  </div>
               </div>
            </div>
            <!--Blog-inner End-->
         </div>
      </div>
      <!-- End of Latest News carousel --> 

      {% endblock main_content %}
